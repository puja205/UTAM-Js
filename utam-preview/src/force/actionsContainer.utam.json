{
    "elements": [
        {
            "name": "actionButton",
            "type": "utam-force/pageObjects/actionButton",
            "public": true,
            "selector": {
                "css": ".forceActionButton",
                "returnAll": true
            },
            "filter": {
                "apply": "getLabel",
                "matcher": {
                    "type": "stringEquals",
                    "args": [
                        {
                            "name": "text",
                            "type": "string"
                        }
                    ]
                },
                "findFirst": true
            }
        },
        {
            "name": "actionLink",
            "type": "utam-force/pageObjects/actionLink",
            "public": true,
            "selector": {
                "css": ".forceActionLink",
                "returnAll": true
            },
            "filter": {
                "apply": "getLabel",
                "matcher": {
                    "type": "stringEquals",
                    "args": [
                        {
                            "name": "text",
                            "type": "string"
                        }
                    ]
                },
                "findFirst": true
            }
        }
    ]
}
