{
    "elements": [
        {
            "type": "container",
            "public": true,
            "name": "content"
        },
        // to add
        {
            "type": "container",
            "public": true,
            "name": "contentInsideSlot",
            "selector": {
                "css": ":not(slot)"
            }
        },
        // to add
        {
            "type": "container",
            "public": true,
            "name": "generatedTemplate",
            "selector": {
                "css": ".forcegenerated-flexipage-template"
            }
        }
    ],
    "methods": [
        {
            "name": "waitForTemplate",
            "compose": [
                {
                    "apply": "waitFor",
                    "args": [
                        {
                            "type": "function",
                            "predicate": [
                                {
                                    "element": "root",
                                    "apply": "containsElement",
                                    "args": [
                                        {
                                            "type": "locator",
                                            "value": {
                                                "css": ".forcegenerated-flexipage-template"
                                            }
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                },
                {
                    "apply": "returnSelf"
                }
            ]
        }
    ]
}
