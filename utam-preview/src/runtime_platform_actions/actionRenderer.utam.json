{
    "shadow": {
        "elements": [
            {
                "name": "lightningButton",
                "type": "utam-lightning/pageObjects/button",
                "public": true,
                "selector": {
                    "css": "lightning-button"
                }
            },
            {
                "name": "headlessAction",
                "type": "utam-action/pageObjects/executorLwcHeadless",
                "public": true,
                "selector": {
                    "css": "runtime_platform_actions-executor-lwc-headless"
                }
            }
        ]
    },
    "methods": [
        {
            "name": "clickButton",
            "compose": [
                {
                    "element": "lightningButton"
                },
                {
                    "chain": true,
                    "apply": "click"
                }
            ]
        }
    ]
}
