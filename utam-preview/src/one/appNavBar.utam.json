{
    "shadow": {
        "elements": [
            {
                "name": "navItem",
                "type": "utam-global/pageObjects/appNavBarItemRoot",
                "public": true,
                "selector": {
                    "css": "one-app-nav-bar-item-root",
                    "returnAll": true
                },
                "filter": {
                    "apply": "getItemText",
                    "matcher": {
                        "type": "stringContains",
                        "args": [
                            {
                                "name": "text",
                                "type": "string"
                            }
                        ]
                    },
                    "findFirst": true
                }
            }
        ]
    }
}
